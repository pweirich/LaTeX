@phdthesis{Podesva06,
	Abstract = {This work challenges the traditional assumption that phonological variables are categorical in nature. Variationists typically investigate the social and linguistic factors that condition the use of particular categories of variant, often ignoring the considerable phonetic variation within categories. I argue here that phonetic details play a central role in constructing social meaning and that they can also participate in the linguistic motivation of variation. Phonological variables can vary along a host of phonetic dimensions, dimensions that can be manipulated to intensify meaning onto create new meanings.

Recent approaches to variation view linguistic features not as markers of social categories but rather as symbolic resources that encode the meanings from which social categories are derived. Following such approaches, I propose a theoretical framework for analyzing the link between linguistic forms and their social meanings. The framework is applied to phonetic and phonological variation patterns in the speech of three gay professionals as they move from one situation to another.

The question of whether phonetic detail matters permeates the analysis, and to show that the significance of phonetic detail is broad, I examine variation at three levels of representation: the segmental level (realization of word-final coronal stops), the prosodic level (intonational variation in declaratives), and voice quality (falsetto phonation). Robust and socially meaningful patterns of phonetic variation emerge for all three variables, often in the absence of patterned categorical variation.

The dissertation concludes by bringing the three variables together, along with their proposed social meanings, to consider how they are assembled to construct styles. It is suggested that while individual linguistic features index vague meanings on their own, the meaning of sets of features is considerably more vivid.

This work illustrates that an approach taking into account the phonetic details of variation can bring to the surface a rich palate of meanings that cannot be accessed by categorical investigations alone. If one of the goals of variation studies is to investigate the construction of social meaning, the scope of traditional approaches must expand to incorporate more phonetic axes of variation.},
	Author = {Podesva, Robert Jay},
	Date-Added = {2015-09-29 16:19:50 +0000},
	Date-Modified = {2015-09-29 16:19:50 +0000},
	Keywords = {Communication and the arts, Language, literature and linguistics, Phonetic detail, Sociolinguistic variation, Linguistic, Social meaning, Gay},
	School = {Stanford University},
	Title = {Phonetic detail in sociolinguistic variation: Its linguistic significance and role in the construction of social meaning},
	Type = {Dissertation},
	Year = {2006}}

@phdthesis{Niedzielski1997,
	Abstract = {The goal of this research is to determine to what extent listeners use social information about a speaker in constructing phonological space. The main hypotheses are the following: (1) listeners use social information to calibrate the phonological space of speakers; (2) stereotypes about given language varieties effect the way in which listeners calibrate the phonological space of speakers of those varieties; and (3) people's stereotypes about their own variety can be inaccurate, and that the phonological space calibrated for members of their own speech communities reflects this inaccuracy.

Forty-one Detroit-area residents were given perceptual tests in which they were asked to choose from a set of resynthesized vowels the tokens that they felt best-matched the vowels they heard in the speech of a fellow Detroiter. Half of the respondents were told that the speaker was from Detroit, while half were told that she was from Windsor, Ontario, Canada, which is directly across the Detroit River from Detroit. A language attitudes survey in Detroit had indicated that Detroiters stereotype Canadian speech, particularly with regard to the raised dipthong as in the phrase 'aboot the hoose.' Nevertheless, acoustic analysis of Detroiters' speech indicated that this raised diphthong is present in the speech of many Detroiters.

Both groups of respondents heard the same Detroit female speaker on the test tape. However, respondents given the CANADIAN label chose the raised-diphthong tokens as those present in the dialect of the speaker, while those given the MICHIGAN label did not choose raised diphthongs.

Respondents given the MICHIGAN label also chose vowels that corresponded to Standard American vowels, despite the fact that the Northern Cities Chain Shift has drastically shifted the vowels of Detroit speakers away from such a canonical system.

Since the "speaker's" nationality was the only aspect of the test that varied between the two groups of respondents, this label alone must have caused the difference in the selection of tokens. This indicates that listeners do indeed use social information in the perception of speech.},
	Author = {Nancy Niedzielski},
	Date-Added = {2015-09-29 16:19:32 +0000},
	Date-Modified = {2019-07-20 14:09:37 -0400},
	Keywords = {Psychology, Language, literature and linguistics, American dialects},
	School = {University of California, Santa Barbara},
	Title = {The effect of social information on the phonetic perception of sociolinguistic variables},
	Year = {1997}}

@phdthesis{Campbell-Kibler2005,
	Abstract = {Understanding the structure of sociolinguistic variation requires understanding what information it coveys to listeners and how. Variation not only correlates with social structures but carries social meaning, influencing listener perceptions and through them social structures. This research demonstrates that listeners' interpretations of the English variable (ING) (working/workin') hadepends on contextual factors and its correlates interact with each other perceptually.

The study employs the Matched Guise Technique, using digitally manipulated stimuli differing only in tokens of (ING). The stimuli were created from spontaneous speech from eight speakers, two men and two women each from the South and the West Coast. Listener reactions to the manipulated recordings were collected in two phases. Group interviews (N = 60) provided open-ended information regarding the recordings generally and listener ideologies about (ING). This data was used to construct a survey (N = 124) to collect more structured responses.

The findings show that (ING) is connected to a network of social meanings, including education, articulateness, formality, region and the rural/urban divide. Overall, speakers were rated as more educated and more likely to be articulate when they used -ing . The data also show that variation-based social meaning is not compositional but instead the impact of a variable depends on its context. Regional accents triggered expectations regarding (ING) use and meaning, which influenced the impact of actual uses. (ING) shifted the relationship between pairs of meanings, such as masculinity and intelligence or socioeconomic class and formality. Finally, listeners showed agency selecting what cues to attend to and how to interpret them. The effect of (ING) depended, among other things, on the positive or negative reaction of the listener. Using - ing could make a speaker sound more intelligent to some while making others think only that the speaker is trying to sound more intelligent.

These results show that variation does influence listener perceptions, i.e. carries social meanings. These meanings, however, are not static. To understand their structure, a fully developed theory of style is needed, explaining not only how variation contributes to larger impressions, but also how overall impressions influence the contribution of a single variable.},
	Author = {Campbell-Kibler, Kathryn},
	Date-Added = {2015-09-29 16:19:17 +0000},
	Date-Modified = {2015-09-29 16:19:17 +0000},
	Keywords = {Language, literature and linguistics, Listener, Sociolinguistic, ING, Variationist linguistics},
	School = {Stanford University},
	Title = {Listener perceptions of sociolinguistic variables: The case of ({ING})},
	Year = {2005}}


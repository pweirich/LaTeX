@mastersthesis{Jeon2012,
	Abstract = {Perceptual dialectology studies have shown that people have strong opinions about the number and placement of dialect regions. There has been relatively little research conducted in this area on Korean, however, with early studies using only short language attitude surveys. The present study uses the `draw-a-map' task to examine perceptions of language variation in Korea. Respondents were asked to draw a line around places in Korea where people speak differently and provide names, examples, and comments about the language spoken in those areas. These data were aggregated and analyzed with ArcGIS 10.0 software and using the `keywords' method. An analysis of these data suggest that Koreans' perceptions of dialect regions are not necessarily limited by administrative boundaries; in fact, the data reveal not only perceptions of dialect variation unassociated with geographic borders, but also taps into the way people connect ideas about language and place. Results from this study have implications for language attitudes research, perceptual dialectology methodology, and understanding the relationship between language and place in Korea.},
	Author = {Jeon, Lisa},
	Date-Added = {2017-02-28 04:27:02 +0000},
	Date-Modified = {2017-02-28 04:27:02 +0000},
	File = {:Users/phillipweirich/Library/Application Support/Mendeley Desktop/Downloaded/Jeon - 2012 - No Title.pdf:pdf},
	Keywords = {perceptual dialectology},
	School = {University of North Texas},
	Title = {Drawing Boundaries And Revealing Language Attitudes: Mapping Perceptions Of Dialects in Korea},
	Type = {Masters Thesis},
	Year = {2012}}

@article{Lameli2008,
	Abstract = {Within the context of investigations of cognitive spatial models, the study analyzes the effect of varying the informational structure of linguistic survey instruments. A group of informants was asked to draw German language areas on different types of geographic maps. First, in a quantitative approach we show how the specific informational structure of each map type influences the frequency and prominence of the named language areas. The second part of the study focuses on the localization of the mentioned language areas. Here we discuss the specific effects of each type of map and the information it contains on the localization. We also discuss general characteristics of the conceptualization of linguistic space as well as differences in the perception of the prominent language areas.},
	Author = {Lameli, Alfred and Purschke, Christoph and Kehrein, Roland},
